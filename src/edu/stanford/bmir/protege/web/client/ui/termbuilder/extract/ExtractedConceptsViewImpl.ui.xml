<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.termVisCanvas {
		width: 100%;
	    height: 100%;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" height="" styleName="tb-button-bar">
			<g:Button text="Select All" ui:field = "selectAllButton" styleName="tb-button"></g:Button>
			<g:Button text="Unselect All" ui:field = "unselectAllButton" styleName="tb-button"></g:Button>
			<g:Button text="Add Into Class Tree" ui:field = "acceptButton" styleName="tb-button" addStyleNames="tb-button-special"></g:Button>
		</g:HorizontalPanel>
		<table width="100%">
		<!--<g:HTMLPanel ui:field="termVisCanvas" styleName="{style.termVisCanvas}"></g:HTMLPanel>-->
            <g:HTMLPanel ui:field="termVisCanvas"></g:HTMLPanel>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 