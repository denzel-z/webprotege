<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" styleName="tb-button-bar">
			<g:Button text="Create" ui:field = "createButton" styleName="tb-button"></g:Button>
			<g:Button text="Delete" ui:field = "deleteButton" styleName="tb-button"></g:Button>
			<g:Button text="Clear All" ui:field = "clearButton" styleName="tb-button"></g:Button>
			<g:Button text="Generate" ui:field = "generateButton" styleName="tb-button" addStyleNames="tb-button-special"></g:Button>
		</g:HorizontalPanel>
		<p1:DataGrid width="100%" height="220px" ui:field="dataGrid"/>
	</g:HTMLPanel>
</ui:UiBinder> 