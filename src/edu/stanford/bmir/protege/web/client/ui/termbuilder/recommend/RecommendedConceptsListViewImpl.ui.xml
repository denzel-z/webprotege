<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.cell.client" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
        <g:HorizontalPanel ui:field="anchorTextBar" width="100%" horizontalAlignment="ALIGN_LEFT" styleName="tb-anchor-text-bar">
            <g:Anchor ui:field="textAnchor" styleName="tb-anchor-text" />
        </g:HorizontalPanel>
		<p1:DataGrid width="100%" height="220px" ui:field="dataGrid"/>
        <g:HorizontalPanel ui:field="buttonBar" width="100%" horizontalAlignment="ALIGN_LEFT" height="" styleName="tb-button-bar">
            <g:Button text="Add Into Class Tree" ui:field = "acceptButton" styleName="tb-button"/>
        </g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 