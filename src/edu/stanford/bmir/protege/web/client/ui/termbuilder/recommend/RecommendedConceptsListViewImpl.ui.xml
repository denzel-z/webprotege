<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.cell.client" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
    .anchor-text {
        color: #175AA9;
        font-size: 14px;
        margin: 5px;
        cursor: pointer;
        cursor: hand;
        text-decoration: underline;
    }
	</ui:style>
	<g:HTMLPanel>
        <g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" height="">
            <g:Anchor ui:field="anchor" styleName="{style.anchor-text}" />
        </g:HorizontalPanel>
		<p1:DataGrid width="100%" height="225px" ui:field="dataGrid"/>
        <g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" height="">
            <g:Button text="Add Into Class Tree" ui:field = "acceptButton"/>
        </g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 